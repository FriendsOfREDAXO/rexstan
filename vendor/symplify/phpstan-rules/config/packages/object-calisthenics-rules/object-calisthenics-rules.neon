services:
    -
        class: Symplify\PHPStanRules\ObjectCalisthenics\Rules\NoShortNameRule
        tags: [phpstan.rules.rule]
        arguments:
            minNameLength: 3
            allowedShortNames: ['id', 'to', 'up']

rules:
    - Symplify\PHPStanRules\ObjectCalisthenics\Rules\NoChainMethodCallRule
